{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    { "src": "/api/contacts", "dest": "/server.js" },
    { "src": "/api/users", "dest": "/server.js" },
    { "src": "/(.*)", "dest": "/server.js" }
  ],
  "env": {
    "PORT": 5000
  }
}
